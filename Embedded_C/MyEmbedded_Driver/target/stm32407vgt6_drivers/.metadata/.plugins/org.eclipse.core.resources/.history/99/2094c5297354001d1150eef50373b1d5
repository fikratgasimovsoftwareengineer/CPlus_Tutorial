#ifndef STM32FXX_SPIO_DRIVER_H_
#define STM32FXX_SPIO_DRIVER_H_



typedef struct{

	uint8_t SPI_DeviceMod;
	uint8_t SPI_BusConfig;
	uint8_t SPI_SclkSpeed;
	uint8_t SPI_DFF;
	uint8_t SPI_CPHA;
	uint8_t SPI_CPOL;
	uint8_t SPI_SSM;

}SPI_Config_t;









#endif /* STM32FXX_SPIO_DRIVER_H_ */
