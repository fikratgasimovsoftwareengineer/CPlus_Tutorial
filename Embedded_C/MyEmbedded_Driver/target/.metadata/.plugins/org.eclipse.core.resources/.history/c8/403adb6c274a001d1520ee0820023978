#include <stdint.h>

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

//Define ADC Clock Peripheral
#define ADC_CLC_BASE 		0x40012000UL

#define ADC_CLC_OFFSET 		0x04

#define ADC_CR1_REG_ADDR		(ADC_CLC_BASE + ADC_CLC_OFFSET)



int main(void)
{

	uint32_t *p_AddCR1Reg = (uint32_t*)ADC_CR1_REG_ADDR;
	*p_AddCR1Reg |= (1 << 8);


}
