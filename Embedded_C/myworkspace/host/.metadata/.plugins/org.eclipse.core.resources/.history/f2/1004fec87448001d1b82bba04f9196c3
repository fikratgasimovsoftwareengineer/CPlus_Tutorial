#include <stdio.h>
#include <stdint.h>

struct MyModel{

	uint32_t carNum;

	uint32_t carPrice;

	uint16_t carMaxSpeed;

	float carWeight;

};


void structPointer(struct MyModel *pStruct);


int main(){

	struct MyModel data;

	struct MyModel CarBMW = {2021, 15000, 220, 1330};

	struct MyModel CarFiat = {2019, 13000, 110, 1240};

	structPointer(&data);


}

void structPointer(struct MyModel *pStruct){

	//Access Variables with Derefenceing

	printf("Car Max %d\n", pStruct->carMaxSpeed);

	printf("Car Num %d\n", pStruct->carNum);

}
